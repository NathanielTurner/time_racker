<h2>Project List</h2>

<table>
  <thead>
    <tr>
      <th>Project Name</th>
      <th>Time Limit</th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td id="project-name"><%= project.name %></td>
        <td id="time-limit"><%= project.time_limit %></td>
      </tr>
    <% end %>
  </tbody>
</table>
